var count = 0;
var label = 1;
var selected_label = 1;

function sleep(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
}

result = check(label, selected_label)

async function check(label, selected_label){
    for(let i = 0; i < 50; i++){
        if(label == selected_label){
            count += 1
            console.log(count)
            await sleep(60)
        }
    }
    return count
}
